    "/california/pre-licensing/how-to-become-real-estate-agent-california": [
        {
            "@context": "http://schema.org/",
            "@type": "HowTo",
            "name": "How to Become an California Real Estate Agent",
            "description": "5 Steps Toward Getting Your California Salesperson License",
            "image": {
                "@type": "ImageObject",
                "url": "https://www.theceshop.com/california/pre-licensing/how-to-become-real-estate-agent-california",
                "height": "42",
                "width": "216"
            },
            "estimatedCost": {
                "@type": "MonetaryAmount",
                "currency": "USD",
                "value": "129.00"
            },
            "step": [
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/california/pre-licensing/how-to-become-real-estate-agent-california",
                    "name": "Complete 135 hours of Pre-Licensing education.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "You must first take and pass 135 hours of Pre-Licensing coursework, which consists of three courses."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Pass-Licensing.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/california/pre-licensing/how-to-become-real-estate-agent-california",
                    "name": "Pass the course final exam.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "Once you complete each Pre-Licensing course, you must take a final exam. You have two attempts to pass each final exam, and you're allotted one minute per question. The passing score for each of these final exams is 70%, and you must take each final in the presence of a proctor."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Pass-Final.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/california/pre-licensing/how-to-become-real-estate-agent-california",
                    "name": "Pass the California salesperson licensing exam.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "After you pass your course final exams, you'll take the California licensing exam. This exam consists of 150 multiple-choice questions with a time limit of 3 hours and 15 minutes, and the minimum passing score is 70%."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Pass-Licensing.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/california/pre-licensing/how-to-become-real-estate-agent-california",
                    "name": "Choose a sponsoring broker.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "To begin your new career, you must be sponsored by an active California real estate broker. It’s important to find a good fit when picking a broker to work with and it’s a key factor to becoming a successful California real estate agent."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Select-Broker.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/california/pre-licensing/how-to-become-real-estate-agent-california",
                    "name": "Complete your licensing application.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "Within 90 days of passing your licensing exam, you must submit your official application and any applicable fees, as well as a fingerprint-based federal and state criminal background check. Once the application has been accepted, you'll be issued a temporary license."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Get-Documents-AZ.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/california/pre-licensing/how-to-become-real-estate-agent-california",
                    "name": "Complete your licensing application.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "After you pass the examination, and if you have not filed a License Combo Application, then you will be sent a Salesperson License Application (RE 202). This form must be completed and returned along with the appropriate fee within one year of passing the examination. A license cannot be issued until DRE receives fingerprint response information."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Contract-Class.png",
                        "height": "300",
                        "width": "400"
                    }
                }
            ],
            "totalTime": "P135H"
        },
        {
            "@context": "https://schema.org",
            "@type": "VideoObject",
            "name": "How to Get Your Real Estate License in California | The CE Shop",
            "description": "Getting your California real estate license might seem daunting, but we’re here to guide you through the process. You can learn about the five steps you’ll need to complete before starting your career in California real estate. Prerequisites include: Being eighteen years of age; applicants must be considered honest and truthful; and applicants must disclose any criminal history. Failure to disclose may result in disciplinary action. Step One: Complete 135-hours of approved Pre-Licensing education. Step Two: Pass the course final exam. Step Three: Pass the California salesperson licensing exam. Step Four: Choose a sponsoring broker. Step Five: Complete your licensing application.",
            "thumbnailUrl": [
                "https://www.theceshop.com/sites/default/files/block_image/videos/CA-Thumbnail.jpg"
            ],
            "uploadDate": "2019-08-7",
            "duration": "PT2M51S",
            "embedUrl": "https://www.youtube.com/embed/YH-6mqJwUt0",
            "interactionStatistic": {
                "@type": "InteractionCounter",
                "interactionType": {
                    "@type": "http://schema.org/WatchAction"
                },
                "userInteractionCount": 800
            }
        }
    ],
    "/colorado/pre-licensing/how-to-become-real-estate-agent-colorado": [
        {
            "@context": "http://schema.org/",
            "@type": "HowTo",
            "name": "How to Become a Colorado Real Estate Agent",
            "description": "6 Steps Toward Getting Your Colorado Broker License",
            "image": {
                "@type": "ImageObject",
                "url": "https://www.theceshop.com/colorado/pre-licensing/how-to-become-real-estate-agent-colorado",
                "height": "42",
                "width": "216"
            },
            "estimatedCost": {
                "@type": "MonetaryAmount",
                "currency": "USD",
                "value": "459.00"
            },
            "step": [
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/colorado/pre-licensing/how-to-become-real-estate-agent-colorado",
                    "name": "Complete an approved 168 hour Pre-Licensing course.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "Successfully complete an approved 168 clock hour Pre-Licensing course. Courses may be completed online depending on the program."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Complete-Pre.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/colorado/pre-licensing/how-to-become-real-estate-agent-colorado",
                    "name": "Pass the course final exam.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "At the end of each course, you must take and pass a course final exam. Depending on the course length, each course final exam consists of either 20 questions or 50 questions. Upon completing the entire 168-hour program, you must take a program final exam. This final exam consists of 100 state-specific questions and 100 national questions, and the minimum passing score is 75%."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Pass-Final.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/colorado/pre-licensing/how-to-become-real-estate-agent-colorado",
                    "name": "Pass the Colorado Real Estate Broker exam.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "Next, you'll take and pass the Colorado Real Estate Broker Licensing Exam, which is administered by PSI. This exam consists of an 80-question National portion with a 120-minute time allowance and a 74-question State portion with a 110-minute time allowance."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Pass-Licensing.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/colorado/pre-licensing/how-to-become-real-estate-agent-colorado",
                    "name": "Complete a background check.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "You must next submit a set of fingerprints to the Colorado Bureau of Investigation. These fingerprints are used to conduct a Colorado and federal fingerprint-based criminal history background check utilizing records from the CBI and the FBI. "
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Criminal-History-Report.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/colorado/pre-licensing/how-to-become-real-estate-agent-colorado",
                    "name": "Acquire errors and omissions (E&O) insurance.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "You must have an E&O insurance policy in place before obtaining an active license."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/E%26O-Insurance.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/colorado/pre-licensing/how-to-become-real-estate-agent-colorado",
                    "name": "Complete the application.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "The Colorado Division of Real Estate’s Online Services uses eLicense for new licensee candidates to apply for their broker license, and for existing licensees to manage their license."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Get-Registered.png",
                        "height": "300",
                        "width": "400"
                    }
                }
            ],
            "totalTime": "P168H"
        }
    ],
