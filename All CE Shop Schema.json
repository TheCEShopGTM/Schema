                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "Contact Pearson VUE at www.pearsonvue.com to schedule the state and national examinations. Be prepared to provide to the exam administrator two forms of current signature identification, including one government issued I.D. with photo and the Salesperson Education Certificate for the 60 hours of schooling."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Pass-Final.png",
                        "height": "300",
                        "width": "400"
                    }
                },
                {
                    "@type": "HowToStep",
                    "url": "https://www.theceshop.com/arkansas/pre-licensing/how-to-become-real-estate-agent-arkansas",
                    "name": "Apply for your license.",
                    "itemListElement": [
                        {
                            "@type": "HowToDirection",
                            "text": "Salesperson License applicants must submit all pages of a successful score report from the test, a license fee of $50.00, recovery fund fee of $25.00, and a signed post-license requirement notice. The score report and fees must be received within ninety days of the examination date."
                        }
                    ],
                    "image": {
                        "@type": "ImageObject",
                        "url": "https://www.theceshop.com/sites/default/files/schema/Select-Broker.png",
                        "height": "300",
                        "width": "400"
                    }
                }
